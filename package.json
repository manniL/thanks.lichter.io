{
  "name": "thanks.lichter.io",
  "version": "1.2.0",
  "description": "A small one-page to provide a donation option for people who want to support and thank me",
  "author": "Alexander Lichter <npm@lichter.io>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "generate": "nuxt generate && yarn lambda-build",
    "lambda-dev": "cross-env NODE_ENV=development yarn lambda-build && cross-env NODE_ENV=development yarn lambda-serve",
    "lambda-build": "netlify-lambda build ./functions",
    "lambda-serve": "netlify-lambda serve ./functions",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "@nuxtjs/tailwindcss": "^1.1.2",
    "consola": "^2.10.1",
    "cross-env": "^6.0.3",
    "nuxt-edge": "^2.10.0-26175435.15fa12fc",
    "nuxt-purgecss": "^0.2.1",
    "nuxt-svg-loader": "^1.0.1",
    "postcss-nested": "^4.1.2",
    "stripe": "^7.9.1",
    "tailwindcss": "^1.1.2",
    "vue-confetti": "^2.0.5",
    "vue-next-level-scroll": "^1.2.0",
    "vue-stripe-elements-plus": "^0.3.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "1.1.2",
    "@nuxtjs/proxy": "^1.3.3",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.2",
    "husky": "^3.0.8",
    "netlify-lambda": "^1.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
